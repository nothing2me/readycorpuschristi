{
  "version": 2,
  "builds": [
    {
      "src": "api/vercel.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/vercel.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "api/vercel.py"
    },
    {
      "src": "/mapzone/(.*)",
      "dest": "api/vercel.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/vercel.py"
    }
  ]
}
