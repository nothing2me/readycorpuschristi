{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/vercel.py"
    },
    {
      "source": "/static/(.*)",
      "destination": "/api/vercel.py"
    },
    {
      "source": "/mapzone/(.*)",
      "destination": "/api/vercel.py"
    },
    {
      "source": "/(.*)",
      "destination": "/api/vercel.py"
    }
  ]
}

